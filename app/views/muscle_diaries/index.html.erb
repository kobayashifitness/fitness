<div class="page-header">
  <%= link_to new_muscle_diary_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Muscle diary
  <% end %>
  <h1>Listing muscle_diaries</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @muscle_diaries) do |muscle_diary| %>
            <td><%= link_to 'Show', muscle_diary %></td>
        <td><%= link_to 'Edit', edit_muscle_diary_path(muscle_diary) %></td>
        <td><%= link_to 'Destroy', muscle_diary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
