<div class="page-header">
  <%= link_to new_protein_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Protein
  <% end %>
  <h1>Listing proteins</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @proteins) do |protein| %>
            <td><%= link_to 'Show', protein %></td>
        <td><%= link_to 'Edit', edit_protein_path(protein) %></td>
        <td><%= link_to 'Destroy', protein, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
