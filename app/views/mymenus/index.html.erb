<div class="page-header">
  <%= link_to new_mymenu_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Mymenu
  <% end %>
  <h1>Listing mymenus</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @mymenus) do |mymenu| %>
            <td><%= link_to 'Show', mymenu %></td>
        <td><%= link_to 'Edit', edit_mymenu_path(mymenu) %></td>
        <td><%= link_to 'Destroy', mymenu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
